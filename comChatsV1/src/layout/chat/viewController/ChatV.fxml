           xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <VBox SplitPane.resizableWithParent="false">
        <HBox>
            <children>
                <Button mnemonicParsing="false" prefHeight="65.0" prefWidth="120.0" text="Insert Username"/>
                <Button mnemonicParsing="false" prefHeight="65.0" prefWidth="120.0" text="Add friend"/>
            </children>
        </HBox>
        <ListView fx:id="lvFriends" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
        </ListView>
    </VBox>
    <VBox prefHeight="398.0" prefWidth="392.0"/>
